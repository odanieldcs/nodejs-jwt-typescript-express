{
  "name": "nodejs-jwt-typescript-express",
  "version": "1.0.0",
  "description": "A tutorial from middleware for authentication using Express and TypeScript",
  "main": "src/index.js",
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/app.ts' -e ts",
    "start": "NODE_ENV=production node ./dist/app.js",
    "build": "rm -rf ./dist && ttsc -p tsconfig-build.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
